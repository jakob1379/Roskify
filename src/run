#!/usr/bin/env bash
set -euo pipefail
IFS=$'\n\t'

echo "
##########################################################################
# Extracting artists                                                     #
##########################################################################"
./rfArtistURL

echo "
##########################################################################
# Conerting to spotify URI                                               #
##########################################################################"
./rfArtistURL2spotifyURI

echo "
##########################################################################
# updating spotify playlist                                              #
##########################################################################"
python listCreation.py -n "1130349271" -p "1L7qcjzDKSdtXNZuhztl2z"
